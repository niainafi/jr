{
    "redirects": [
      {
        "source": "/wp-admin/:path*",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/wp-content/:path*",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/blog/:path*",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/old-site/:path*",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/:path*",
        "has": [
          {
            "type": "header",
            "key": "x-vercel-cache",
            "value": "HIT"
          }
        ],
        "destination": "/:path*",
        "statusCode": 200
      }
    ],
    "headers": [
      {
        "source": "/wp-admin/:path*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0"
          }
        ]
      }
    ]
  }
  